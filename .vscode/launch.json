  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Start Server",
      "runtimeExecutable": "babel-node",
      "runtimeArgs": ["server/server.js"]
    },
    {
      "type": "node",
      "request": "launch",
      "name": "launch file",
      "program": "${file}"
