    {
      "type": "node",
      "request": "launch",
      "name": "Launch via Babel (works)",
      "cwd": "${workspaceRoot}",
      "port": 9229,
      "program": "",
      "runtimeExecutable": "yarn",
      "console": "integratedTerminal",
      "runtimeArgs": ["start:server"]
    }
